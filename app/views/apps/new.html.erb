<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">New iOS App</h1>
  </div>
  <!-- /.col-lg-12 -->
</div>
<div class="row">
  <div class="col-lg-12">
    <%= form_for @app do |f| %>
      <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name, class: 'form-control', placeholder: 'App name' %>
      </div>
      <div class="form-group">
        <%= f.label :uid %>
        <%= f.text_field :uid, class: 'form-control', placeholder: 'Apple ID' %>
      </div>
      <%= f.hidden_field :type, value: 'App::Ios' %>
      <%= f.submit 'Create', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
