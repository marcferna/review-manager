<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header"><%= @app.name %> Reviews</h1>
  </div>
  <!-- /.col-lg-12 -->
</div>
<div class="row">
  <div class="col-lg-12">
    <table class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th>Title</th>
          <th>Text</th>
          <th>Rating</th>
          <th>Version</th>
          <th>User</th>
          <th>Date</th>
        </tr>
      </thead>
      <tbody>
        <%= render @reviews %>
      </tbody>
    </table>
    <!-- /.table-responsive -->
  </div>
  <!-- /.col-lg-12 -->
</div>
